Public Function Format0_00(curImporte As Currency, Optional ByRef intError As Integer) As String
    On Error GoTo Error_Format0_00
    intError = False
    Dim strAux As String, i As Integer
    strAux = Format(curImporte, "0.00")
    i = InStr(strAux, ",")
    If i > 0 Then
        Mid(strAux, i, 1) = "."
    End If
    Format0_00 = strAux
Salir_Format0_00:
    Exit Function
Error_Format0_00:
    Select Case Err
        Case Else
            MsgBox "error nº " & Err & " en Format0_00" & vbCrLf & Err.Description
            intError = True
            Resume Salir_Format0_00
    End Select
End Function