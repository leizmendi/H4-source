Private Sub PasarTabla(rs As Recordset, strFile As String, intTipo As Integer, j As Integer)
    On Error GoTo Error_PasarTabla
    Dim i As Integer
    Open strFile For Output As #1
    While Not rs.EOF
        For i = 0 To rs.Fields.Count - 2
            Print #1, Llena(rs(i), rs.Fields(i).Properties("Size"));
        Next i
        If intTipo = 1 Then
            Print #1,
        End If
        rs.MoveNext
        j = j + 1
    Wend
    Close 1
Salir_PasarTabla:
    Exit Sub
Error_PasarTabla:
    Select Case Err
        Case Else
            MsgBox "Error nº " & Err & " en PasarTabla" & vbCrLf & Err.Description
            Resume Salir_PasarTabla
    End Select
End Sub

