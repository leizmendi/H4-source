    gintVentaServicio = glngIdFichaCab = 0 'Nz(DimeXML(strXML, "VentaServicioSN"), "N") = "S"
    CargarParam Me, Me.Name & "_"
    CurrentDb.Execute "UPDATE qrycfgParam SET VPbool = False WHERE NP Like 'ListaServicios+*'", dbFailOnError
    Carga_lstServicios
