        Me.Caption = "Servicios en Ficha: " & glngIdFichaCab
        Me.casCobrar.Visible = False
    Else
        Me.Caption = "Servicios auxiliares"
        Me.casCobrar.Visible = True
        Me.casCobrar = True
    End If
