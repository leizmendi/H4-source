    Dim y As Long, z As Long
    Set rs = CurrentDb.OpenRecordset("tbClientesOV", dbOpenDynaset)
    If Not rs.EOF Then
        rs.MoveLast
        rs.MoveFirst
    End If
    z = rs.RecordCount
    While Not rs.EOF
        DoEvents
        y = y + 1
        SysCmd acSysCmdSetStatus, "Arreglando teléfono Clientes " & y & "/" & z
        rs.Edit
        j = 0
        For i = 1 To 4
            sTel = Nz(rs("Tel" & i), "")
            If sTel <> "" Then
                sTel = Trim(sTel)
                If sTel Like "[67]*" Then
                    sTel = Replace(sTel, " ", "")
                    sTel = "34" & sTel
                    If Len(sTel) > 11 Then sTel = Left(sTel, 11)
                    j = j + 1
                    rs("Telefono" & j) = sTel
                    k = k + 1
                    If j = 2 Then Exit For
                End If
            End If
        Next i
        rs.Update
        rs.MoveNext
    Wend
    MsgBox "Se asignaron " & k & " teléfonos para " & z & " clientes", vbInformation
HandleExit:
    Exit Sub
HandleError:
    MsgBox Err.Description
    Resume HandleExit
End Sub

Public Sub ArreglaNombreClientes()
    On Error GoTo HandleError
    Dim rs As Recordset, sNombre As String, i As Integer, j As Integer, k As Integer
    Dim y As Long, z As Long
    Set rs = CurrentDb.OpenRecordset("tbClientesOV", dbOpenDynaset)
    If Not rs.EOF Then
        rs.MoveLast
        rs.MoveFirst
    End If
    z = rs.RecordCount
    While Not rs.EOF
        DoEvents
        y = y + 1
        SysCmd acSysCmdSetStatus, "Arreglando Nombre Clientes " & y & "/" & z
        rs.Edit
        sNombre = Nz(rs("Cliente"), "")
        If sNombre <> "" Then
            sNombre = Trim(sNombre)
            i = InStr(sNombre, " ")
            If i > 0 Then
                sNombre = Left(sNombre, i - 1)
                sNombre = UCase(Left(sNombre, 1)) & LCase(Mid(sNombre, 2))
                rs("Nombre") = sNombre
                k = k + 1
            End If
        End If
        rs.Update
        rs.MoveNext
    Wend
    MsgBox "Se asignaron " & k & " nombres para " & z & " clientes", vbInformation
HandleExit:
    Exit Sub
HandleError:
    MsgBox Err.Description
    Resume HandleExit
End Sub

Public Function EnviadoMensaje(lIdMensaje As Long, lIdCliente As Long) As String
